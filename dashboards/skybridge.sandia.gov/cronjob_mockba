#This cronjob runs on Irina's machine, mockba.  Basically the skybridge results are copied to this machine,
#then copied to software-login.  This is because I cannot get scp to work on skybridge from a cronjob.
00 00 * * 1-7  rm -rf /home/ikalash/skybridgeCDash/HOMMEXX/Skybridge*/*
50 00 * * 1-7  scp -r ikalash@ceerws2801b.sandia.gov:/gpfs1/ikalash/hommexxCDash/SkybridgeTrilinosSerial/20* /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeTrilinosSerial; scp -r /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeTrilinosSerial/20* ikalash@software-login.sandia.gov:/home/ikalash/hommexxCDash/SkybridgeTrilinosSerial
00 01 * * 1-7  scp -r ikalash@ceerws2801b.sandia.gov:/gpfs1/ikalash/hommexxCDash/SkybridgeHOMMEXXSerial/20* /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeHOMMEXXSerial; scp -r /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeHOMMEXXSerial/20* ikalash@software-login.sandia.gov:/home/ikalash/hommexxCDash/SkybridgeHOMMEXXSerial
00 03 * * 1-7  scp -r ikalash@ceerws2801b.sandia.gov:/gpfs1/ikalash/hommexxCDash/SkybridgeTrilinosOpenMP/20* /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeTrilinosOpenMP; scp -r /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeTrilinosOpenMP/20* ikalash@software-login.sandia.gov:/home/ikalash/hommexxCDash/SkybridgeTrilinosOpenMP
01 03 * * 1-7  scp -r ikalash@ceerws2801b.sandia.gov:/gpfs1/ikalash/hommexxCDash/SkybridgeHOMMEXXOpenMP/20* /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeHOMMEXXOpenMP; scp -r /home/ikalash/skybridgeCDash/HOMMEXX/SkybridgeHOMMEXXOpenMP/20* ikalash@software-login.sandia.gov:/home/ikalash/hommexxCDash/SkybridgeHOMMEXXOpenMP
